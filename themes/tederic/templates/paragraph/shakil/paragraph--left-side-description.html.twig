{% set file_link = file_url(paragraph.field_download_upload.entity.fileuri) %}

<div class="automo lens-sec1 br-inj">
    <div class="container-fluid">
        <div class="row">
            <div class="col-md-6 col-sm-12 col-xs-12">
                {{ content.field_section_title|raw }}
                 {{ content.field_description|raw }}
                {{ content.field_text_|raw }}
                <a class="broucher" href="{{ file_link }}">{{ content.field_lin|raw }}<span><i class="fa fa-download"></i></span></a>
            </div>
            <!--Slider Section Start-->
            <div class="col-md-6 col-sm-12 col-xs-12">
                <div class="slider-area-main">
                    <div id="myCarousel" class="carousel slide" data-ride="carousel">
                        <div class="carousel-inner">

                            <div class="item">
                                <img src="images/big-inj-big.png" alt="big-inj-big" style="width:100%;">
                            </div>

                            <div class="item">
                                <img src="images/big-inj-big.png" alt="big-inj-big" style="width:100%;">
                            </div>

                            <div class="item active">
                                <img src="images/big-inj-big.png" alt="big-inj-big" style="width:100%;">
                            </div>

                        </div>

                        <!-- Left and right controls -->
                        <!--<a class="left carousel-control" href="#myCarousel" data-slide="prev">
      <img src="images/slarrow-left.png">
    </a>-->
                        <div class="lens-arow">
                            <ol class="carousel-indicators">
                                <li data-target="#myCarousel" data-slide-to="0" class="">1/4</li>
                                <li data-target="#myCarousel" data-slide-to="1">2/4</li>
                                <li data-target="#myCarousel" data-slide-to="2" class="active">3/4</li>
                            </ol>
                            <a class="right carousel-control" href="#myCarousel" data-slide="next">
                                <img src="images/slarrow-right.png">
                            </a>
                        </div>
                    </div>
                </div>
            </div>
            <!--Slider Section End-->

        </div>
    </div>
</div>
